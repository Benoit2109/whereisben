Vignettes\";\nimport styles from \"./Exploring.module.css\";\n\nexport default function Exploring() {\n  const [citysearch, setCitysearch] = useState(\"\");\n  const { cities, setCities } = useContext(CitiesContext);\n  const [cityLoop, setcityLoop] = useState([]);\n\n  useEffect(() => {\n    axios\n      .get(`${process.env.REACT_APP_API_BDD}cities`)\n      .then((res) => res.data)\n      .then((res) => {\n        setCities(res);\n      });\n  }, [setCities]);\n\n  const FilterCity = cityLoop\n    .filter(\n      (e) =>\n        e.city_name.includes(citysearch) || e.country_name.includes(citysearch)\n    )\n    .map((city) => <Vignettes key={city.id} city={city} />);\n\n  useEffect(() => {\n    setcityLoop(cities);\n  }, []);\n\n  return (\n    <div className={styles.exploringdisplay}>\n      <Map />\n      <div className={styles.exploringcenter}>\n        <SearchBar citysearch={citysearch} setCitysearch={setCitysearch} />\n        <div className={styles.exploring_vignettes_wrapper}>\n        {FilterCity.length > 0 ? (\n          FilterCity\n        ) : (\n          <p className={styles.nocities}>\n            Nous n'avons pas trouvé de ville(s) correspondant à votre recherche.\n          </p>\n        )}\n        </div>\n      </div>\n    </div>\n  );\n}\n","/Users/benoitlebourdonnec/dev/whereisben/src/Components/Views/Homepage/Homepage.jsx",["110"],"import React, {useContext, useEffect} from 'react';\nimport axios from 'axios';\n\nimport VideoPlayer from '../../Video/VideoPlayer';\nimport { CitiesContext } from '../../Context/CitiesContext';\n\nexport default function Homepage() {\n    const {setCities} = useContext(CitiesContext)\n    useEffect(() => {\n        axios\n          .get(`${process.env.REACT_APP_API_BDD}cities`)\n          .then((res) => res.data)\n          .then((res) => {\n            setCities(res);\n          });\n      }, []);\nreturn(\n    <div>\n        <VideoPlayer />\n    </div>\n)\n}","/Users/benoitlebourdonnec/dev/whereisben/src/Components/AddCity/AddCity.jsx",["111"],"import React, { useEffect } from \"react\";\nimport axios from \"axios\";\nimport PropTypes from \"prop-types\";\nimport styles from \"./AddCity.module.css\";\nimport { Link } from \"react-router-dom\";\n\nexport default function AddCity({ newcity, setNewcity }) {\n  // je récupère le fichier image contenu dans l'input type file.\n  // je l'insère dans l'objet Newcity.\n  const onchangePhoto = (e) => {\n    if (e.target.files[0]) {\n      setNewcity({ ...newcity, [e.target.name]: e.target.files[0] });\n    } else {\n      setNewcity({ ...newcity, [e.target.name]: \"\" });\n    }\n  };\n  // je récupère toutes les valeurs de mes champs pour les insérer dans mon objet newCity.\n  const onChange = (e) => {\n    setNewcity({ ...newcity, [e.target.name]: e.target.value });\n  };\n\n  const onSubmit = (e) => {\n    e.preventDefault();\n\n    // je reformate mes données pour les envoyer vers ma base de données et pouvoir récupérer ma photo en type file.\n    const data = new FormData();\n    data.append(\"photo\", newcity.photo);\n    data.append(\"country_name\", newcity.country_name);\n    data.append(\"city_name\", newcity.city_name);\n    data.append(\"longitude\", newcity.longitude);\n    data.append(\"latitude\", newcity.latitude);\n    data.append(\"nb_visited\", newcity.nb_visited);\n    data.append(\"last_visited\", newcity.last_visited);\n\n    // j'envoi la ville dans la base de donnée.\n    axios\n      .post(`${process.env.REACT_APP_API_BDD}cities/newcity`, data)\n      .then((res) => res.data)\n      .then((res) => {\n        alert(\"La ville à bien été ajouté !\");\n      })\n      .catch((e) => {\n        console.error(e.message);\n      });\n    // je réinitialise mon objet newCity à null pour ajouter une nouvelle ville.\n    setNewcity({\n      country_name: \"\",\n      city_name: \"\",\n      latitude: \"\",\n      longitude: \"\",\n      photo: \"\",\n      last_visited: \"\",\n      nb_visited: \"\",\n    });\n  };\n\n  return (\n    <div className={styles.addcityWrapper}>\n      <div className={styles.addcityformContainer}>\n        <form\n          className={styles.addcityform}\n          action=\"/newcity\"\n          method=\"POST\"\n          enctype=\"multipart/form-data\"\n        >\n          <label htmlFor=\"country_name\">\n            Pays:\n            <input\n              className={styles.addcityformInput}\n              type=\"text\"\n              name=\"country_name\"\n              placeholder=\"Nom du pays\"\n              value={newcity.country_name}\n              onChange={onChange}\n            />\n          </label>\n          <label htmlFor=\"city_name\">\n            Ville visitée:\n            <input\n              className={styles.addcityformInput}\n              type=\"text\"\n              name=\"city_name\"\n              placeholder=\"Nom de la ville\"\n              value={newcity.city_name}\n              onChange={onChange}\n            />\n          </label>\n          <label htmlFor=\"latitude\">\n            <input\n              className={styles.addcityformInput}\n              type=\"text\"\n              name=\"latitude\"\n              placeholder=\"Latitude\"\n              value={newcity.latitude}\n            />\n          </label>\n          <label htmlFor=\"longitude\">\n            <input\n              className={styles.addcityformInput}\n              type=\"text\"\n              name=\"longitude\"\n              placeholder=\"Longitude\"\n              value={newcity.longitude}\n            />\n          </label>\n          <label htmlFor=\"photo\">\n            Photo:\n            <input\n              className={styles.addcityformInput}\n              type=\"file\"\n              accept=\"image/*\"\n              name=\"photo\"\n              placeholder=\"Url de la photo\"\n              value={newcity.photo.filename}\n              onChange={(e) => onchangePhoto(e)}\n            />\n          </label>\n          <label htmlFor=\"last_visited\">\n            Dernière visite:\n            <input\n              className={styles.addcityformInput}\n              type=\"number\"\n              name=\"last_visited\"\n              placeholder=\"ex: 2017\"\n              value={newcity.last_visited}\n              onChange={onChange}\n            />\n          </label>\n          <label htmlFor=\"nb_visited\">\n            Nbre de visites:\n            <input\n              className={styles.addcityformInput}\n              type=\"number\"\n              name=\"nb_visited\"\n              placeholder=\"ex: 5\"\n              value={newcity.times_visited}\n              onChange={onChange}\n            />\n          </label>\n\n          <label htmlFor=\"submit\" className={styles.addcitybuttoncontainer}>\n            <input\n              type=\"submit\"\n              name=\"submmit\"\n              onClick={(e) => onSubmit(e)}\n              value=\"Envoyer\"\n              className={styles.addcitybutton}\n            />\n          </label>\n        </form>\n      </div>\n      <Link to=\"/exploring\" className={styles.addcityviewButtoncontainer}>\n        <button className={styles.addcityviewButton}>\n          Afficher sur la carte\n        </button>\n      </Link>\n    </div>\n  );\n}\n\nAddCity.Proptype = {\n  newcity: PropTypes.object.isRequired,\n  setNewcity: PropTypes.func.isRequired,\n};\n","/Users/benoitlebourdonnec/dev/whereisben/src/Components/SearchBar/SearchBar.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Navbar/Navbar.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Vignettes/Vignettes.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Video/VideoPlayer.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Map/Map.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Navbar/Menu.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Context/CitiesContext.jsx",[],"/Users/benoitlebourdonnec/dev
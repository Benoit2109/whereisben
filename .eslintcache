;\n\nexport default function Homepage() {\n  const { setCities } = useContext(CitiesContext);\n  const id = localStorage.getItem(\"ID\");\n  useEffect(() => {\n    id &&\n      axios\n        .get(`${process.env.REACT_APP_API_BDD}cities/user/${id}`, {\n          headers: {\n            Authorization: `bearer ${localStorage.getItem(\"TOKEN\")}`,\n          },\n        })\n        .then((res) => res.data)\n        .then((res) => {\n          setCities(res);\n        });\n  }, []);\n  return (\n    <div className={styles.Homepage_wrapper}>\n      <VideoPlayer />\n    </div>\n  );\n}\n","/Users/benoitlebourdonnec/dev/whereisben/src/Components/AddCity/AddCity.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/SearchBar/SearchBar.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Navbar/Navbar.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Vignettes/Vignettes.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Video/VideoPlayer.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Map/Map.jsx",[],"/Users/benoitlebourdonnec/dev/whereisben/src/Components/Navbar/Menu.jsx",["111","112"],"import React, { useState } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport styles from \"./Menu.module.css\";\n\nimport globe from \"../../Assets/globe_menu.png\";\nimport globeConnected from \"../../Assets/globeGreen_menu.png\";\nimport Snackbar from '@material-ui/core/Snackbar';\nimport MuiAlert from '@material-ui/lab/Alert';\n\nexport default function Menu() {\n  const [active, setActive] = useState(false);\n  const [open, setOpen] = useState(false);\n    const [error, setError] = useState(false);\n\n  const openMenu = () => {\n    setActive(!active);\n  };\n\n  const deconnect = () => {\n    localStorage.removeItem(\"TOKEN\");\n    localStorage.removeItem(\"ID\");\n    setOpen(true)\n  };\n\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  function Alert(props) {\n    return <MuiAlert elevation={6} variant=\"filled\" {...props} />;\n  }\n\n  return (\n    <div className={styles.menuWrapper}>\n      <button type=\"button\" className={styles.menuButton} onClick={openMenu}>\n        <img\n          className={styles.menuGlobe}\n          src={localStorage.getItem(\"TOKEN\") ? globeConnected : globe}\n          alt=\"globe\"\n        />\n      </button>\n      <ul className={`${styles.menuList} ${active && styles.menuOpen}`}>\n        <li className={styles.menuListItem} onClick={openMenu}>\n          <Link className={styles.menuLink} to=\"/\">\n            <strong>Homepage</strong>\n          </Link>\n        </li>\n        <li className={styles.menuListItem} onClick={openMenu}>\n          <Link className={styles.menuLink} to=\"/exploring\">\n            <strong>Exploring cities</strong>\n          </Link>\n        </li>\n        <li className={styles.menuListItem} onClick={openMenu}>\n          <Link className={styles.menuLink} to=\"/login\">\n            {localStorage.getItem(\"TOKEN\") ? (\n              <p className={styles.menuP} onClick={() => deconnect()}>\n                <strong>Deconnexion</strong>\n              </p>\n            ) : (\n              <strong>Connexion / Inscription</strong>\n            )}\n          </Link>\n        </li>\n        {localStorage.getItem(\"TOKEN\") ? (\n          <li className={styles.menuListItem} onClick={openMenu}>\n            <Link className={styles.menuLink} to=\"/addcity\">\n              <strong>Add new city</strong>\n            </Link>\n          </li>\n        ) : (\n          \"\"\n        )}\n      </ul>\n      <Snackbar\n        anchorOrigin={{\n          vertical: \"bottom\",\n          horizontal: \"center\",\n        }}\n        open={open}\n        autoHideDuration={2500}\n        onClose={handleClose}\n      >\n        <Alert onClose={handleClose} severity=\"success\">\n          {\"Vous êtes déconnecté.\"}\n        </Alert>\n      </Snackbar>\n    </div>\n  );\n}\n","/Users/benoitlebourdon